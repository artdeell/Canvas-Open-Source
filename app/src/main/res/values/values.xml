<?xml version="1.0" encoding="utf-8"?>
<resources>

    <item name="tag_rikka_material_WindowInsetsHelper" type="id"/>
    <declare-styleable name="WindowInsetsHelper">
        <attr format="boolean" name="edgeToEdge"/>
        <attr format="flags" name="fitsSystemWindowsInsets">
            <flag name="top" value="0x30"/>
            <flag name="bottom" value="0x50"/>
            <flag name="left" value="0x03"/>
            <flag name="right" value="0x05"/>
            <flag name="start" value="0x00800003"/>
            <flag name="end" value="0x00800005"/>
        </attr>
        <attr format="flags" name="consumeSystemWindowsInsets">
            <flag name="top" value="0x30"/>
            <flag name="bottom" value="0x50"/>
            <flag name="left" value="0x03"/>
            <flag name="right" value="0x05"/>
            <flag name="start" value="0x00800003"/>
            <flag name="end" value="0x00800005"/>
        </attr>
        <attr format="flags" name="layout_fitsSystemWindowsInsets">
            <flag name="top" value="0x30"/>
            <flag name="bottom" value="0x50"/>
            <flag name="left" value="0x03"/>
            <flag name="right" value="0x05"/>
            <flag name="start" value="0x00800003"/>
            <flag name="end" value="0x00800005"/>
        </attr>
    </declare-styleable>

    <attr name="colorDisabled" format="color" />
    <attr name="colorDisabledVariant" format="color" />

    <style name="WidgetFoundation.ProgressBar.Indeterminate" parent="Widget.AppCompat.ProgressBar.Horizontal">
        <item name="android:indeterminate">true</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">100dp</item>
        <item name="android:indeterminateTint">?colorPrimary</item>
    </style>
</resources>